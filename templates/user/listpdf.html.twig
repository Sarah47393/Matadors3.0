{% block body %}
    <h1>User index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>CIN</th>
                <th>Role</th>
                <th>Access</th>
                <th>Date naissance</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.Nom }}</td>
                <td>{{ user.Prenom }}</td>
                <td>{{ user.CIN }}</td>
                <td>{{ user.Role }}</td>
                <td>{{ user.Access }}</td>
                <td>{{ user.datenaissance|date('Y-m-d')}}</td>
               
            </tr>
       
        {% endfor %}
        </tbody>
    </table>

  
{% endblock %}
